{
    "manifest_version": 2,
    "name": "The Time Extension",
    "version": "1.0.0",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]

        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": "LOGO.png",
        "title": "extensao"
    }
}